{
  "redirects": [
    {
      "source": "/blog/:blogslug.html",
      "destination": "/blog/:blogslug",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/admin-login",
      "destination": "/admin/admin-login.html"
    },
    {
      "source": "/admin",
      "destination": "/admin/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
